{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "app.zidwell.com"
        }
      ],
      "destination": "/my-app"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "app.zidwell.com"
        }
      ],
      "destination": "/my-app"
    }
  ]
}